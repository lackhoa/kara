(load "set.kar")

(stdisplay-n "In set => #t")
(in-set? 2 (Set (list 1 2 3 4)))

(stdisplay-n "In set with range => #f")
(in-set? 5 (Set (range 1 4)))

(stdisplay-n "Intersection => (Set (1 2 9))")
(seq->list (intersection-set (Set (range 0 10))
                             (Set (list 1 2 9 11))))

(stdisplay-n "Union => (Set (0 ... 11))")
(seq->list (union-set (Set (range 0 10))
                      (Set (list 1 2 9 11))))
