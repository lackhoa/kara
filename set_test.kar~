(load "set.kar")

(stdisplay-n "In set => #t")
(in-set? 2 (list 1 2 3 4))

(stdisplay-n "In set with range=> #f")
(in-set? 5 (range 1 4))

(stdisplay-n "Intersection => (1 2 9)")
(stream->list (intersection-set (range 0 10) (list 1 2 9 11)))

(stdisplay-n "Union => (0 ... 11)")
(stream->list (union-set (range 0 10) (list 1 2 9 11)))
