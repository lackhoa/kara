(set! cadr '(car (cdr)))
(set! cddr '(cdr (cdr)))
(set! caddr '(car (cddr)))
(set! cdddr '(cdr (cddr)))
(set! cadddr '(car (cdddr)))
(set! set-cadr! '(set-car! (cdr $0) $1))
(set! set-caddr! '(set-car! (cddr $0) $1))
(set! set-cadddr! '(set-car! (cdddr $0) $1))
(set! pair? '(not (null?)))

; params: $0 (list), $1 (index)
(set! list-ref
    '(if (= $1 0) (car $0) (list-ref (cdr $0) (- $1 1))))
